    <local:OpeningsPage Title="Openings" IconImageSource="WhiteKnight.png">
    </local:OpeningsPage>
    <local:EventsPage Title="Events" IconImageSource="Events.png">
    </local:EventsPage>
    <local:MorePage Title="More" IconImageSource="more.png">
    </local:MorePage>

</TabbedPage>